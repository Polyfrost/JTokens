{
  "colors": {
    "$type": "color",
    "white": {
      "$value": "#ffffff"
    }
  },
  "button": {
    "other-color": {
      "$value": "{button.button-color}",
      "$type": "color"
    },
    "button-color": {
      "$value": "{colors.white}",
      "$type": "color"
    }
  },
  "Primary font": {
    "$value": "Comic Sans MS",
    "$type": "fontFamily"
  },
  "Body font": {
    "$value": ["Helvetica", "Arial"],
    "$type": "fontFamily"
  },
  "font-weight-default": {
    "$value": 350,
    "$type": "fontWeight"
  },
  "font-weight-thick": {
    "$value": "extra-bold",
    "$type": "fontWeight"
  },
  "rem": {
    "$value": "0.25rem",
    "$type": "dimension"
  },
  "px": {
    "$value": "10px",
    "$type": "dimension"
  },
  "Duration-100": {
    "$value": "100ms",
    "$type": "duration"
  },
  "Duration-200": {
    "$value": "200ms",
    "$type": "duration"
  },
  "Accelerate": {
    "$value": [0.5, 0, 1, 1],
    "$type": "cubicBezier"
  },
  "Decelerate": {
    "$value": [0, 0, 0.5, 1],
    "$type": "cubicBezier"
  },
  "file2-file1": {
    "$type": "color",
    "$value": "{random-color}"
  },
  "priority": {
    "$type": "duration",
    "$value": "1ms"
  }
}
{
  "colors": {
    "$type": "color",
    "white": {
      "$value": "#ffffff",
      "$extensions": {
        "org.example.tool-a": 42
      }
    }
  },
  "button": {
    "other-color": {
      "$value": "{button.button-color}",
      "$type": "color"
    },
    "button-color": {
      "$value": "{colors.white}",
      "$type": "color"
    }
  },
  "Primary font": {
    "$value": "Comic Sans MS",
    "$type": "fontFamily"
  },
  "Body font": {
    "$value": [
      "Helvetica",
      "Arial"
    ],
    "$type": "fontFamily"
  },
  "font-weight-default": {
    "$value": 350,
    "$type": "fontWeight"
  },
  "font-weight-thick": {
    "$value": "extra-bold",
    "$type": "fontWeight"
  },
  "rem": {
    "$value": "0.25rem",
    "$type": "dimension"
  },
  "px": {
    "$value": "10px",
    "$type": "dimension"
  },
  "Duration-100": {
    "$value": "100ms",
    "$type": "duration"
  },
  "Duration-200": {
    "$value": "200ms",
    "$type": "duration"
  },
  "Accelerate": {
    "$value": [
      0.5,
      0,
      1,
      1
    ],
    "$type": "cubicBezier"
  },
  "Decelerate": {
    "$value": [
      0,
      0,
      0.5,
      1
    ],
    "$type": "cubicBezier"
  },
  "string": {
    "$type": "string",
    "$value": "abc"
  },
  "number": {
    "$type": "number",
    "$value": 1.5
  },
  "boolean": {
    "$type": "boolean",
    "$value": true
  },
  "object": {
    "$type": "object",
    "$value": {
      "abc": true
    }
  },
  "array": {
    "$type": "array",
    "$value": [
      "abc"
    ]
  },
  "font": {
    "$type": "typography",
    "main": {
      "$value": {
        "fontFamily": "Roboto",
        "fontSize": "42px",
        "fontWeight": 700,
        "letterSpacing": "1em",
        "lineHeight": "2"
      }
    },
    "main-clone": {
      "$value": "{font.main}"
    },
    "secondary": {
      "$type": "typography",
      "$value": {
        "fontFamily": "{Primary font}",
        "fontSize": "{rem}",
        "fontWeight": "{font-weight-thick}",
        "letterSpacing": "{px}",
        "lineHeight": "1.5"
      }
    }
  },
  "shadow": {
    "$type": "shadow",
    "$value": {
      "color": "#00000000",
      "offsetX": "0.5rem",
      "offsetY": "0.5rem",
      "blur": "1.5rem",
      "spread": "0rem"
    }
  },
  "transition": {
    "$type": "transition",
    "$value": {
      "duration": "200ms",
      "delay": "0ms",
      "timingFunction": [
        0.5,
        0,
        1,
        1
      ]
    }
  },
  "gradient": {
    "$type": "gradient",
    "$value": [
      {
        "color": "#0000ff",
        "position": 0
      },
      {
        "color": "#ff0000",
        "position": 1
      }
    ]
  },
  "dashed": {
    "$type": "strokeStyle",
    "$value": "dashed"
  },
  "custom-dashes": {
    "$type": "strokeStyle",
    "$value": {
      "dashArray": [
        "{dash-length}",
        "0.25rem"
      ],
      "lineCap": "butt"
    }
  },
  "dash-length": {
    "$type": "dimension",
    "$value": "10px"
  },
  "border": {
    "1": {
      "$type": "border",
      "$value": {
        "color": "{colors.white}",
        "width": "3px",
        "style": "solid"
      }
    },
    "2": {
      "$type": "border",
      "$value": {
        "color": "{colors.white}",
        "width": "1px",
        "style": "{custom-dashes}"
      }
    }
  }
}